#include <iostream>

using namespace std;

int main()
{
   int n,i,arr[50],j,temp;
   cout<<"enter total no of elements :";
   cin>>n;
   cout<<"enter"<<n<<"numbers  :";
   for(i=0;i<n:i++)
   {
       cin>>arr[i];
   }
   cout<<"sorting array using bubble sort tech....\n";
   for(i=0;i<(n-1);i++)
   {
       for(j=0;j<(n-i-1);j++)
       {
           if(arr[j]>arr[j+1])
           {
               temp=arr[j];
               arr[j]=arr[j+1];
               arr[j+1]=temp;
           }
       }
   }
   cout<<"elements sorted successfully...\n";
   cout<<"sorted list in ascending order: \n";
   for(i=0;i<n;i++)
   {
       cout<<arr[i]<<"";
   }
           
       
   

    return 0;
}
